    "aws::graph::propositions": [
      "# Meteorological Information and Flight Plan for Accident",
      "The accident site had Visual Meteorological Conditions (VMC).",
      "The condition of light at the accident site was Day.",
      "The observation facility was EMT.",
      "EMT observation facility is at an elevation of 296 feet mean sea level.",
      "The observation facility was 9 Nautical Miles from the accident site.",
      "The observation time was 15:47 Local.",
      "The observation facility was 130° direction from the accident site.",
      "The lowest cloud condition was Few clouds at 4200 feet Above Ground Level.",
      "The visibility was 10 miles.",
      "The lowest ceiling was Overcast at 6000 feet Above Ground Level.",
      "The wind speed was 6 knots.",
      "The wind direction was 180°.",
      "The altimeter setting was 30.05 inches of mercury.",
      "The temperature was 19°C.",
      "The dew point was 14°C.",
      "There was no obscuration at the accident site.",
      "There was no precipitation at the accident site.",
      "The departure point was Altadena, California.",
      "The flight plan filed was Company Visual Flight Rules (VFR).",
      "The destination was Altadena, California.",
      "The type of clearance was None.",
      "The type of airspace was Class E."
    ],
