    "aws::graph::propositions": [
      "# Meteorological Information and Flight Plan for Accident Site",
      "The accident site had Visual Meteorological Conditions (VMC).",
      "The condition of light at the accident site was Day.",
      "The observation facility was KLEF.",
      "KLEF was 15 Nautical Miles from the accident site.",
      "The observation time was 16:58 Local.",
      "The direction from the accident site to the observation facility was 270°.",
      "The lowest cloud condition at the accident site was Clear.",
      "The visibility at the accident site was 10 miles.",
      "The accident site had no ceiling.",
      "The wind speed at the accident site was 11 knots.",
      "The wind direction at the accident site was 210°.",
      "The altimeter setting at the accident site was 29.95 inches Hg.",
      "The temperature at the accident site was 16°C.",
      "The dew point at the accident site was 2°C.",
      "There was no obscuration at the accident site.",
      "There was no precipitation at the accident site.",
      "The departure point was Lonoke county, Arkansas.",
      "No flight plan was filed.",
      "The destination was Lonoke county, Arkansas.",
      "No clearance was obtained for the flight."
    ],
