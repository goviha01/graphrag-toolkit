    "aws::graph::propositions": [
      "# Meteorological Information and Flight Plan Details",
      "The accident site had Visual Meteorological Conditions (VMC).",
      "The condition of light at the accident site was day.",
      "The observation facility was HSV at an elevation of 629 feet mean sea level.",
      "The observation facility was 3 nautical miles from the accident site.",
      "The observation time was 14:53 local time.",
      "The observation facility was located 295 degrees from the accident site.",
      "The lowest cloud condition was clear.",
      "The visibility was 10 miles.",
      "There was no ceiling at the accident site.",
      "The wind speed was 6 knots.",
      "The wind direction was 20 degrees.",
      "The altimeter setting was 30.01 inches of mercury.",
      "The temperature was 14 degrees Celsius.",
      "The dew point was -1 degrees Celsius.",
      "There was no obscuration at the accident site.",
      "There was no precipitation at the accident site.",
      "The departure point was Greenbrier, Alabama.",
      "No flight plan was filed.",
      "The destination was Greenbrier, Alabama.",
      "No clearance was obtained.",
      "The departure time was 16:00 local time."
    ],
