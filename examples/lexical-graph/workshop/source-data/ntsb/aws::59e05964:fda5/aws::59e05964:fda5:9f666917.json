    "aws::graph::propositions": [
      "# Meteorological Information and Flight Plan for Accident",
      "The accident site had Visual Meteorological Conditions (VMC).",
      "The condition of light at the accident site was Day.",
      "The observation facility was MAO at an elevation of 92 feet mean sea level.",
      "The observation time was 08:15 Local.",
      "The lowest cloud condition was Scattered clouds at 7000 feet Above Ground Level.",
      "The visibility at the accident site was 10 miles.",
      "The lowest ceiling was Overcast at 8500 feet Above Ground Level.",
      "The altimeter setting was 29.94 inches of Mercury.",
      "The temperature at the accident site was 23 degrees Celsius.",
      "The dew point at the accident site was 23 degrees Celsius.",
      "There was no obscuration at the accident site.",
      "There was no precipitation at the accident site.",
      "The departure point was Columbia, South Carolina (CAE).",
      "The flight plan filed was Instrument Flight Rules (IFR).",
      "The destination was Marion, South Carolina (MAO).",
      "The type of clearance was Instrument Flight Rules (IFR).",
      "The departure time was 07:15 Local."
    ],
