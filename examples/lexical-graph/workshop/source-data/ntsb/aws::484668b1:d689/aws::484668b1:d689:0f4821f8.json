    "aws::graph::propositions": [
      "# Meteorological Information and Flight Plan for Accident",
      "The accident site had Visual Meteorological Conditions (VMC).",
      "The condition of light at the accident site was dusk.",
      "The observation facility was MNI.",
      "The observation facility was 27 Nautical Miles from the accident site.",
      "The observation time was 17:55 Local.",
      "The observation facility was 190° from the accident site.",
      "The lowest cloud condition was scattered clouds at 6500 feet Above Ground Level (AGL).",
      "The visibility at the accident site was 10 miles.",
      "The lowest ceiling was broken clouds at 11000 feet Above Ground Level (AGL).",
      "The wind speed was 6 knots.",
      "The wind direction was 290°.",
      "The turbulence severity was not applicable.",
      "The altimeter setting was 29.72 inches of mercury (Hg).",
      "The temperature was 27°C.",
      "The dew point was 13°C.",
      "There was no obscuration at the accident site.",
      "There was no precipitation at the accident site.",
      "The departure point was Manning, SC (MNI).",
      "No flight plan was filed.",
      "The destination was Manning, SC (MNI).",
      "No clearance was obtained.",
      "The departure time was 17:30 Local."
    ],
