    "aws::graph::propositions": [
      "# Meteorological Information and Flight Plan for Accident",
      "The accident site had Visual Meteorological Conditions (VMC).",
      "The condition of light at the accident site was Day.",
      "The observation facility was RFD at an elevation of 742 feet mean sea level.",
      "The observation facility was 14 Nautical Miles from the accident site.",
      "The observation time was 15:54 Local.",
      "The observation facility was 210° from the accident site.",
      "The lowest cloud condition was Few clouds at 25000 feet Above Ground Level.",
      "The visibility at the accident site was 10 miles.",
      "There was no ceiling at the accident site.",
      "The wind speed was 6 knots.",
      "The wind direction was 330°.",
      "The altimeter setting was 30.01 inches of mercury.",
      "The temperature was 24°C.",
      "The dew point was 8°C.",
      "There was no obscuration at the accident site.",
      "There was no precipitation at the accident site.",
      "The departure point was Poplar Grove, IL (C77).",
      "No flight plan was filed.",
      "The destination was Rockford, IL (RFD).",
      "No clearance was obtained.",
      "The departure time was 16:00 Local."
    ],
