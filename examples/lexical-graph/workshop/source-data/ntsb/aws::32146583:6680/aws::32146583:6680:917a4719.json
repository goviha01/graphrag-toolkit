    "aws::graph::propositions": [
      "# Meteorological Information and Flight Plan for Accident",
      "The accident site had Visual Meteorological Conditions (VMC).",
      "The light condition at the accident site was dawn.",
      "The observation facility was KOXC at an elevation of 726 feet mean sea level.",
      "The observation facility was 8 nautical miles from the accident site.",
      "The observation time was 09:45 local time.",
      "The observation facility was 195 degrees from the accident site.",
      "The lowest cloud condition was scattered clouds at 3500 feet above ground level.",
      "The visibility at the accident site was 10 miles.",
      "The lowest ceiling was broken clouds at 4500 feet above ground level.",
      "The wind speed was 6 knots with no gusts.",
      "The wind direction was 90 degrees.",
      "No actual turbulence was reported.",
      "The altimeter setting was 30.31 inches of mercury.",
      "The temperature was 16 degrees Celsius.",
      "The dew point was 11 degrees Celsius.",
      "There was no obscuration at the accident site.",
      "There was no precipitation at the accident site.",
      "The departure point was Danbury, Connecticut (DXR).",
      "No flight plan was filed.",
      "The destination was Waterbury, Connecticut (N41).",
      "No clearance was obtained.",
      "The departure time was 08:00 local time.",
      "The airspace was Class G."
    ],
