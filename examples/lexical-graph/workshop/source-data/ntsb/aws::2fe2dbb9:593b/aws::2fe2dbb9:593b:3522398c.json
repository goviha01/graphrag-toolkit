    "aws::graph::propositions": [
      "# Meteorological Information and Flight Plan Details",
      "The accident site had Visual Meteorological Conditions (VMC).",
      "The condition of light at the accident site was Day.",
      "The observation facility was GCN at an elevation of 6609 feet mean sea level.",
      "The observation facility was 7 Nautical Miles from the accident site.",
      "The observation time was 14:54 Local.",
      "The observation facility was located 280° from the accident site.",
      "The lowest cloud condition was Clear.",
      "The visibility was 10 miles.",
      "There was no ceiling at the accident site.",
      "The wind speed was 8 knots.",
      "The wind direction was 292°.",
      "The altimeter setting was 29.89 inches of mercury (Hg).",
      "The temperature was 16°C.",
      "The dew point was -26°C.",
      "There was no obscuration at the accident site.",
      "There was no precipitation at the accident site.",
      "The departure point was Grand Canyon, Arizona (KGN).",
      "The flight plan filed was Company Visual Flight Rules (VFR).",
      "The destination was Grand Canyon, Arizona (KGN).",
      "No clearance was issued for the flight.",
      "The departure time was 11:41 Local."
    ],
