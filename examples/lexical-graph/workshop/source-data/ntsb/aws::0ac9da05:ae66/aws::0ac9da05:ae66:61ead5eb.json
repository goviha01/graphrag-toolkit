    "aws::graph::propositions": [
      "# Meteorological Information and Flight Plan Details",
      "The conditions at the accident site were Visual Meteorological Conditions (VMC).",
      "The observation facility was KDQH.",
      "KDQH is located at an elevation of 257 feet mean sea level.",
      "The observation time was 13:15 Local.",
      "The lowest cloud condition was Clear.",
      "There was no ceiling.",
      "The wind speed was 7 knots.",
      "The wind direction was 60 degrees.",
      "The altimeter setting was 30.14 inches of mercury.",
      "There was no obscuration at the accident site.",
      "There was no precipitation at the accident site.",
      "The type of flight plan filed was Unknown.",
      "The type of clearance was Unknown.",
      "The departure time was not specified.",
      "The airspace was Class G.",
      "The condition of light was Day.",
      "The accident site was 9 Nautical Miles from the observation facility.",
      "The observation facility was 57 degrees from the accident site.",
      "The visibility was 10 miles.",
      "There was no turbulence forecast.",
      "There was no actual turbulence.",
      "The turbulence severity forecast was Not Applicable.",
      "The turbulence severity actual was Not Applicable.",
      "The temperature was 18 degrees Celsius.",
      "The dew point was 16 degrees Celsius."
    ],
