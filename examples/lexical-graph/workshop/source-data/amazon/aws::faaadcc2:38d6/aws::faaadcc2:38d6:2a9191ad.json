    "aws::graph::propositions": [
      "# Amazon Facilities as of December 31, 2024",
      "Amazon operated 29,551 thousand square feet of leased office space in North America.",
      "Amazon operated 9,104 thousand square feet of owned office space in North America.",
      "Amazon operated 23,771 thousand square feet of leased office space internationally.",
      "Amazon operated 1,802 thousand square feet of owned office space internationally.",
      "Amazon operated 23,975 thousand square feet of leased physical stores in North America.",
      "Amazon operated 707 thousand square feet of owned physical stores in North America.",
      "Amazon operated 222 thousand square feet of leased physical stores internationally.",
      "Amazon operated no owned physical stores internationally.",
      "Amazon operated 448,168 thousand square feet of leased fulfillment, data centers, and other facilities in North America.",
      "Amazon operated 36,869 thousand square feet of owned fulfillment, data centers, and other facilities in North America.",
      "Amazon operated 176,287 thousand square feet of leased fulfillment, data centers, and other facilities internationally.",
      "Amazon operated 18,958 thousand square feet of owned fulfillment, data centers, and other facilities internationally.",
      "Amazon operated a total of 701,974 thousand square feet of leased facilities.",
      "Amazon operated a total of 67,440 thousand square feet of owned facilities."
    ],
